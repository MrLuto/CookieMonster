{
  "name": "CookieMonster",
  "description": "A chrome extension that will delete your cookies",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png",
    "256": "logo.png"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "tabs"
  ]
}
